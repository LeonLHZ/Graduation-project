<script type="text/javascript">
$(function(){
	$.post("${pageContext.request.contextPath}/student?method=getDorList",function(data){
		$(data).each(function(){
			$("#table").append(
				"<tr>"+
				"<td width='4%' align='center' bgcolor='#EEEEEE'>"+
				this.num+
				"</td>"+
				"<td width='4%' align='center' bgcolor='#EEEEEE'>"+
				this.integral+
				"</td>"+
				"</tr>"
			);
		});
		
	},"json");
	
	$.post("${pageContext.request.contextPath}/student?method=getRequest",function(data){
		$(data).each(function(){
			$("#tabl").append(
					"<tr>"+
					"<td width='4%' align='center' bgcolor='#EEEEEE'>"+
					this.rStudent.name+
					"</td>"+
					"<td width='4%' align='center' bgcolor='#EEEEEE'>"+
					this.rStudent.dormitory.num+
					"</td>"+
					"<td width='4%' align='center' bgcolor='#EEEEEE'>"+
					this.rStudent.position.numbering+
					"</td>"+
					"<td width='4%' align='center' bgcolor='#EEEEEE'>"+
					"<a  href='${pageContext.request.contextPath}/student?method=bsdeal&rid="+this.rid+"&code=1'>"+
					"同意"+
					"</a>|"+
					"<a  href='${pageContext.request.contextPath}/student?method=bsdeal&rid="+this.rid+"&code=0'>"+
					"拒绝"+
					"</a>"+
					"</td>"+
					"</tr>"
				);
		});
		
	},"json");
	
	
	
	
	$.post("${pageContext.request.contextPath}/student?method=findAllAnnouncement",function(data){
		$(data).each(function(){
			$("#divhtm").append(
					"<div><div>"+this.content+"</div>"+
					"<div>发布时间：<span>"+this.releasetime+"</span>发布人：<span>"+this.admin.name+"</span></div><HR></div>"
			);
		});
	},"json");
	
	
});

</script>
